<aiml version="1.0.1" encoding="UTF-8">

  <category>
      <pattern>HELLO</pattern>
      <template>
          Well, hello!
      </template>
  </category>

  <category>
    <pattern>MY NAME IS *</pattern>
      <template>
        You have an interesting name, <set name="name"><star/></set>
      </template>
  </category>



  <category>
    <pattern>BOT NAME</pattern>
    <template>
      My name is J.A.R.V.I.S
    </template>
  </category>

  <category>
    <pattern>* YOUR NAME *</pattern>
    <template>
      <srai>BOT NAME</srai>
    </template>
  </category>


    <category>
      <pattern>* YOUR NAME</pattern>
      <template>
        <srai>BOT NAME</srai>
      </template>
    </category>

  <category>
    <pattern>* CALL YOU *</pattern>
    <template>
      <srai>BOT NAME</srai>
    </template>
  </category>



  <category>
    <pattern>BOT INFO</pattern>
    <template>I am fine</template>
  </category>

  <category>
