    </template>
  </category>




  <category>
    <pattern>BYE</pattern>
      <template>
        <random>
          <li>Bye.</li>
          <li>Goodbye.</li>
          <li>Bye bye.</li>
          <li>Bye for now.</li>
          <li>See you later <get name="name"/>.</li>
          <li>Thanks for chatting <get name="name"/>.</li>
        </random>
      </template>
    </category>

  <category>
    <pattern>* BYE</pattern>
    <template>
      <srai>BYE</srai>
    </template>
  </category>

  <category>
    <pattern>BYE *</pattern>
    <template>
      <srai>BYE</srai>
    </template>
  </category>

  <category>
    <pattern>* BYE *</pattern>
    <template>
      <srai>BYE</srai>
    </template>
  </category>


</aiml>
